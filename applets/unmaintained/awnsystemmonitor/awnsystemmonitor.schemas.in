<gconfschemafile>

    <schemalist>

      <schema>
        <key>/schemas/apps/awn-cpumeter/prefs/graph_color</key>
        <owner>awn-cpumeter</owner>
        <type>string</type>
        <default>B39AE6</default>
        <locale name="C">
          <short>The color used in the graph lines.</short>
          <long>The color used in the graph lines.  Valid values are 0.0 through 1.0.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/awn-cpumeter/prefs/border_color</key>
        <owner>awn-cpumeter</owner>
        <type>string</type>
        <default>FFFFFF</default>
        <locale name="C">
          <short>The color used in the applet border.</short>
          <long>The color used in the applet border.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/awn-cpumeter/prefs/border_width</key>
        <owner>awn-cpumeter</owner>
        <type>float</type>
        <default>2.0</default>
        <locale name="C">
          <short>The pixel width of the applet border.</short>
          <long>The pixel width of the applet border.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/awn-cpumeter/prefs/do_gradient</key>
        <owner>awn-cpumeter</owner>
        <type>bool</type>
        <default>0</default>
        <locale name="C">
          <short>Set to 1 (true) to draw the gradient overlay on the graph.</short>
          <long>Set to 1 (true) to draw the gradient overlay on the graph.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/awn-cpumeter/prefs/update_frequency</key>
        <owner>awn-cpumeter</owner>
        <type>int</type>
        <default>1000</default>
        <locale name="C">
          <short>Graph update frequency, in milliseconds.</short>
          <long>Graph update frequency, in milliseconds.</long>
        </locale>
      </schema>

    </schemalist>

</gconfschemafile>
