[DEFAULT]
single_instance = false

[DEFAULT/URI]
type = string
default = http://wiki.awn-project.org/Web_Applet
_description=URI

[DEFAULT/HTML_engine]
type = string
default = webkit
_description=HTML engine to use.

[DEFAULT/width]
type = string
default = 33%%
_description=Width in %% or px.

[DEFAULT/height]
type = string
default = 33%%
_description=height in %% or px.

[DEFAULT/enable_location_dialog]
type = boolean
default = false
_description=Flag that enables usage of the "Open Location" dialog in the applet.

[DEFAULT/first_start]
type = boolean
default = true
_description=If true display location entry and website buttons

[DEFAULT/site]
type = string
default = ;
_description=Name of specialized website to use

