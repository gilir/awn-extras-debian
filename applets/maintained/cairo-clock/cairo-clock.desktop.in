[Desktop Entry]
Version=1.0
_Name=Cairo Clock
Type=Application
X-AWN-Type=Applet
X-AWN-AppletType=Python
_Comment=Displays an analog clock and supports additional clocks for different locations
Exec=awn-applet -p %k
X-AWN-AppletExec=cairo-clock/cairo-clock.py
Icon=cairo-clock/cairo-clock-logo
X-AWN-Applet-Category=Utility,Clock
