[Desktop Entry]
Version=1.0
_Name=Media Player
Type=Application
X-AWN-Type=Applet
X-AWN-AppletType=Python
_Comment=Plays any media files you drop on the applet
Exec=awn-applet -p %k
X-AWN-AppletExec=media-player/media_player.py
Icon=media-player/icons/media-player
X-AWN-AppletCategory=AudioVideo,Audio,Video,Player
