[DEFAULT]
single_instance = false

[DEFAULT/left-click-action]
type = string
default = Show Docklet
_description=Action to perform when left clicking on the icon.

[DEFAULT/log-out-command]
type = string
default = xfce4-session-logout
_description=Command to execute if org.gnome.SessionManager is not available.

[DEFAULT/lock-screen-command]
type = string
default = xscreensaver-command
_description=Command to execute if org.gnome.ScreenSaver is not available.
